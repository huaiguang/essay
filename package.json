  "description": "informal essay",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint src/ --ext .js,.vue --quiet",
    "eslint:fix": "eslint src/ --ext .js,.vue --fix --quiet",
    "prettier:check": "prettier --check \"src/**/*.{vue,js,css}\"",
    "prettier:write": "prettier --write \"src/**/*.{vue,js,css}\"",
    "webpack:test": "cross-env NODE_ENV=development webpack --config build/webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --config build/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production node build/build.js",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue,css,md,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{vue,js}": "eslint --fix"
  },
  "author": "carlos",
  "private": false,
  "repository": {
